{
    "source": {
        "directories": [
            "src"
        ],
        "excludes": [
            "Util/Extractor/ClassExtractor.php",
            "Util/TypeCaster.php"
        ],
    },
    "timeout": 10,
    "logs": {
        "text": "infection.log"
    },
    "mutators": {
        "@default": true,
        "global-ignoreSourceCodeByRegex": [
            "Flag::.*"
        ],
        "CastString": {
            ignore: [
                "Temkaa\\SimpleContainer\\Util\\Env::get"
            ]
        },
        "Continue_": {
            ignore: [
                "Temkaa\\SimpleContainer\\Repository\\DefinitionRepository::findOneByAlias",
                "Temkaa\\SimpleContainer\\Validator\\Definition\\DuplicatedAliasValidator::validate"
            ]
        },
        "DecrementInteger": {
            ignore: [
                "Temkaa\\SimpleContainer\\Service\\Definition\\Configurator\\DecoratorConfigurator::configure",
                "Temkaa\\SimpleContainer\\Service\\Definition\\Configurator\\DecoratorConfigurator::sortByPriority"
            ]
        },
        "GreaterThan": {
            ignore: [
                "Temkaa\\SimpleContainer\\Service\\Definition\\Configurator\\DecoratorConfigurator::sortByPriority"
            ]
        },
        "NullSafeMethodCall": {
            ignore: [
                "Temkaa\\SimpleContainer\\Service\\Definition\\Configurator\\InterfaceConfigurator::collectUnboundInterfaces"
            ]
        },
        "IncrementInteger": {
            ignore: [
                "Temkaa\\SimpleContainer\\Service\\Definition\\Configurator\\DecoratorConfigurator::sortByPriority"
            ]
        },
        "MethodCallRemoval": {
            ignore: [
                "Temkaa\\SimpleContainer\\Service\\Definition\\Configurator\\Argument\\InterfaceConfigurator::configure",
                "Temkaa\\SimpleContainer\\Service\\Definition\\Configurator\\BaseConfigurator"
            ]
        },
        "NewObject": {
            ignore: [
                "Temkaa\\SimpleContainer\\Service\\Definition\\ArgumentConfigurator::configureInterfaceArgument"
            ]
        },
        "PublicVisibility": {
            ignore: ["*"]
        },
        "Throw_": {
            ignore: [
                "Temkaa\\SimpleContainer\\Service\\Definition\\Configurator::configureDefinition",
                "Temkaa\\SimpleContainer\\Service\\Definition\\Resolver::resolveDefinition",
                "Temkaa\\SimpleContainer\\Util\\ExpressionParser::parse"
            ]
        },
        "TrueValue": {
            ignore: [
                "Temkaa\\SimpleContainer\\Util\\Flag"
            ]
        },
        "UnwrapArrayValues": {
            ignore: [
                "Temkaa\\SimpleContainer\\Repository\\DefinitionRepository::__construct",
                "Temkaa\\SimpleContainer\\Model\\Definition\\ClassDefinition::addTags",
                "Temkaa\\SimpleContainer\\Model\\Definition\\ClassDefinition::setAliases",
                "Temkaa\\SimpleContainer\\Model\\Definition\\ClassDefinition::setImplements",
                "Temkaa\\SimpleContainer\\Service\\Definition\\Configurator\\InterfaceConfigurator::addInterfaceDefinitions"
            ]
        },
        "UnwrapArrayUnique": {
            ignore: [
                "Temkaa\\SimpleContainer\\Model\\Definition\\ClassDefinition::addTags",
                "Temkaa\\SimpleContainer\\Model\\Definition\\ClassDefinition::setImplements"
            ]
        }
    },
    "minMsi": 100
}
